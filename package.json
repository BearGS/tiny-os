{
  "name": "tiny-os",
  "version": "0.0.11",
  "description": "a tool",
  "main": "dist/tos.umd.js",
  "module": "dist/tos.esm.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "build": "NODE_ENV=production rimraf dist & node scripts/rollup/index.js",
    "test-build": "NODE_ENV=development node scripts/rollup/index.js",
    "start": "NODE_ENV=development node example/server.js & node scripts/webpack/start.js",
    "lint:fix": "NODE_ENV=production eslint --fix .js,.jsx,.ts,.tsx src  --ignore-pattern '!.eslintrc.js'",
    "lint": "eslint -f 'stylish' --ext .js,.jsx,.ts,.tsx src  --ignore-pattern '!.eslintrc.js'",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/runtime": "^7.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "is-plain-object": "^2.0.4",
    "jest": "^23.6.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "lint-staged": "^7.2.2",
    "live-server": "^1.2.0",
    "ora": "^3.0.0",
    "pre-commit": "^1.2.2",
    "react": "^16.4.2",
    "react-dev-utils": "^11.0.4",
    "react-hot-loader": "^4.3.12",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.1",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "uglify-es": "^3.3.9",
    "webpack": "^4.25.1",
    "webpack-config": "^7.5.0",
    "webpack-dev-server": "^3.1.7"
  },
  "repository": {
    "type": "git",
    "url": "tiny-os"
  },
  "keywords": [
    "tiny-os"
  ],
  "author": "xgs",
  "license": "MIT",
  "jest": {
    "testRegex": "src/__test__/[^/]*(\\.js|\\.coffee|[^d]\\.ts)$",
    "globals": {
      "__DEV__": true,
      "__TEST__": true
    }
  },
  "pre-commit": {
    "run": "lint-staged",
    "silent": true
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "jest --bail --findRelatedTests",
      "git add"
    ]
  },
  "browserlist": {
    "chrome": "50",
    "ios": "10"
  }
}
